			"core/navigation": {
				"elements": {
					"link": {
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/paragraph": {
				"css": ".has-background {padding: 20px 30px;}",
